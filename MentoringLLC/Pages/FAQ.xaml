<Page x:Class="MentoringLLC.Pages.FAQ"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MentoringLLC.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="FAQ">

    <DockPanel LastChildFill="False">
        <TextBox x:Name="Headline" Height="100" FontSize="70" TextAlignment="Center" IsReadOnly="True" DockPanel.Dock="Top" Width="400" >FAQ</TextBox>

        <ListBox  x:Name="listbQuestions" DockPanel.Dock="Top" Width="600" Height="600" Margin="20"></ListBox>
    </DockPanel>
</Page>
